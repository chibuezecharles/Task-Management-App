{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static"
      },
      {
        "src": "api/json-server/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/json-server/(.*)",
        "dest": "/api/json-server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      }
    ]
  }
  