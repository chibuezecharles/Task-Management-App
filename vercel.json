{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      },
      {
        "src": "src/json-server/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/json-server/(.*)",
        "dest": "/src/json-server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      }
    ]
  }
  